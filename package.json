{
  "name": "baseexpocrossapp",
  "version": "0.0.1",
  "expoVersion": {
    "android": 1,
    "ios": 1
  },
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "NODE_ENV=local expo start",
    "android": "NODE_ENV=local expo start --android",
    "ios": "NODE_ENV=local expo start --ios",
    "web": "NODE_ENV=local expo start --web",
    "test": "jest --watchAll",
    "build:web": "NODE_ENV=production expo export:web",
    "build:android": "NODE_ENV=production eas build -p android",
    "build:ios": "NODE_ENV=production eas build -p ios",
    "submit:android": "NODE_ENV=production eas submit -p android",
    "submit:ios": "NODE_ENV=production eas submit -p ios",
    "update:android": "NODE_ENV=production eas update -p android",
    "update:ios": "NODE_ENV=production eas update -p ios",
    "postinstall": "yarn-dedupe && patch-package",
    "check-types": "tsc -p tsconfig.json --noemit",
    "lint": "yarn eslint && yarn check-types",
    "lint:fix": "eslint ./**/*.ts ./**/*.tsx --fix",
    "prettier:fix": "prettier --write App.tsx ./**/*.ts ./**/*.tsx",
    "prettier:check": "prettier --check App.tsx ./**/*.ts ./**/*.tsx",
    "prepare": "husky install",
    "predeploy": "rm -rf web-build && yarn build:web",
    "deploy": "vercel",
    "release": "vercel --prod",
    "prerelease": "rm -rf web-build && yarn build:web"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@bothrs/expo-mixpanel-analytics": "^2.0.0",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/native-stack": "^6.8.0",
    "@sentry/react-native": "4.2.2",
    "consistencss": "^1.7.0",
    "date-fns": "^2.29.2",
    "expo": "~46.0.9",
    "expo-application": "~4.2.2",
    "expo-constants": "~13.2.4",
    "expo-device": "~4.3.0",
    "expo-linking": "~3.2.2",
    "expo-localization": "~13.1.0",
    "expo-splash-screen": "~0.16.2",
    "expo-status-bar": "~1.4.0",
    "expo-updates": "~0.14.5",
    "firebase": "^9.9.4",
    "formik": "^2.2.9",
    "i18n-js": "^4.1.1",
    "mixpanel-browser": "^2.45.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.5",
    "react-native-event-listeners": "^1.0.7",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-web": "~0.18.7",
    "sentry-expo": "~5.0.0",
    "yup": "^0.32.11",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@types/mixpanel-browser": "^2.38.0",
    "@types/react": "^17.0.38",
    "@types/react-native": "~0.69.1",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.0",
    "husky": "^8.0.1",
    "patch-package": "^6.4.7",
    "typescript": "^4.6.3",
    "yarn-dedupe": "^0.2.3"
  },
  "private": true
}
