{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native",
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "#components/*": ["components/*"],
      "#constants": ["helpers/constants"],
      "#contexts/*": ["contexts/*"],
      "#helpers/*": ["helpers/*"],
      "#hooks/*": ["hooks/*"],
      "#images/*": ["assets/images/*"],
      "#navigation/*": ["navigation/*"],
      "#screens/*": ["screens/*"],
      "#services": ["services/index"],
      "#store": ["store/index"],
      "#types": ["types.ts"]
    }
  },
  "exclude": [
    "node_modules",
    "web-build",
  ]
}
